{
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "recreateWhen": "always",
  "onboarding": false,
  "requireConfig": false,
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard"
  ],
  "packageRules": [
    {
      "groupName": "Pin build container images",
      "matchFileNames": [
        "eng/pipelines/common/templates/pipeline-with-resources.yml"
      ],
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": ["mcr.microsoft.com/dotnet-buildtools/prereqs"],
      "pinDigests": true,
      "versioning": "loose"
    }
  ]
}
